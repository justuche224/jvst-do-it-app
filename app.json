{
  "expo": {
    "name": "jvst-do-it",
    "slug": "jvst-do-it",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/android/play_store_512.png",
    "scheme": "jvst-do-it",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/icons/android/res/mipmap-xxxhdpi/ic_launcher.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "monochromeImage": "./assets/icons/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
        "backgroundImage": "./assets/icons/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jvst-tech.todo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/web/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/android/play_store_512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "998a8cea-f74b-4d86-8522-6443cfae06d4"
      }
    }
  }
}
